<p style="color: green"><%= notice %></p>

<h1>Posts</h1>

<div id="posts">
    


  <table class="table">
  <thead>
    <tr>
      <th scope="col">content</th>
      <th scope="col">posted</th>
    </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
    <tr>
      <th scope="row"><%= post.content %></th>
      <td><%= post.created_at.strftime("%r %D") %></td>
    </tr>
  <% end %>
  </tbody>
</table>
</div>

